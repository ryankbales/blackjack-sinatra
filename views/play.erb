<h2>Hello <%= "#{session[:player_name]}" %></h2>
<% players_hand = session[:player_cards] %>
<% dealers_hand = session[:dealer_cards] %>

<h3>Player's Hand</h3>
<ul>
  <% players_hand.each do |card| %>
    <li><%= "#{card}" %></li>
  <% end %>
</ul>

<h3>Dealer's Hand</h3>
<ul class='hand'>
  <% dealers_hand.each do |card| %>
    <li><%= "#{card}" %></li>
  <% end %>
</ul>